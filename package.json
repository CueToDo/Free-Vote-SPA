{
  "name": "free-vote",
  "version": "11.5.11",
  "scripts": {
    "ng": "ng",
    "start": "ng serve -o --watch",
    "pwa": "http-server -p 8080 -c-1 dist/free-vote",
    "build": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:ssr": "ng build --prod && ng run free-vote:server:production",
    "serve:ssr": "node dist/free-vote/server/main.js",
    "ssr": "npm run build:ssr && npm run serve:ssr",
    "prerender-dynamic": "ng run free-vote:prerender --routes-file dynamic-routes.txt",
    "prerender": "ng run free-vote:prerender",
    "dev:ssr": "ng run free-vote:serve-ssr --port 7027",
    "start:heroku": "node dist/free-vote/server/main.js",
    "heroku-postbuild": "npm run build:ssr"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^11.2.0",
    "@angular/cdk": "^11.2.0",
    "@angular/common": "^11.2.0",
    "@angular/compiler": "^11.2.0",
    "@angular/core": "^11.2.0",
    "@angular/flex-layout": "^11.0.0-beta.33",
    "@angular/forms": "^11.2.0",
    "@angular/material": "^11.2.0",
    "@angular/platform-browser": "^11.2.0",
    "@angular/platform-browser-dynamic": "^11.2.0",
    "@angular/platform-server": "^11.2.0",
    "@angular/router": "^11.2.0",
    "@angular/service-worker": "^11.2.0",
    "@auth0/auth0-spa-js": "^1.13.6",
    "@nguniversal/express-engine": "^11.1.2",
    "express": "^4.15.2",
    "hammerjs": "^2.0.8",
    "lodash-es": "^4.17.20",
    "ngx-bootstrap": "^6.2.0",
    "ngx-device-detector": "^2.0.5",
    "rxjs": "~6.6.0",
    "tslib": "^2.1.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1100.7",
    "@angular/cli": "^11.2.0",
    "@angular/compiler-cli": "^11.2.0",
    "@nguniversal/builders": "^11.1.2",
    "@types/express": "^4.17.0",
    "@types/jasmine": "^3.6.3",
    "@types/lodash-es": "^4.17.4",
    "@types/node": "^12.20.0",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.1.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "^4.1.5"
  },
  "browser": {
    "fs": false
  },
  "engines": {
    "node": "14.x"
  }
}
