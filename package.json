{
  "name": "free-vote",
  "version": "14.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve -o --watch",
    "pwa": "http-server -p 7027 -c-1 dist/free-vote/browser",
    "build": "ng build --configuration production",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "eslint . --ext .ts",
    "e2e": "ng e2e",
    "build:ssr": "ng build && ng run free-vote:server",
    "serve:ssr": "node dist/free-vote/server/main.js",
    "ssr": "npm run build:ssr && npm run serve:ssr",
    "prettier-format": "prettier --config .prettierrc \"src/**/*.ts\" --write",
    "prerender-dynamic": "ng run free-vote:prerender --routes-file dynamic-routes.txt",
    "prerender": "ng run free-vote:prerender",
    "dev:ssr": "ng run free-vote:serve-ssr --port 7027",
    "start:heroku": "node dist/free-vote/server/main.js",
    "heroku-postbuild": "npm run build:ssr"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.0.4",
    "@angular/cdk": "^14.0.4",
    "@angular/common": "^14.0.0",
    "@angular/compiler": "^14.0.0",
    "@angular/core": "^14.0.0",
    "@angular/flex-layout": "^14.0.0-beta.40",
    "@angular/forms": "^14.0.0",
    "@angular/material": "^14.0.4",
    "@angular/platform-browser": "^14.0.0",
    "@angular/platform-browser-dynamic": "^14.0.0",
    "@angular/platform-server": "^14.0.4",
    "@angular/pwa": "^14.0.4",
    "@angular/router": "^14.0.0",
    "@angular/service-worker": "^14.0.4",
    "@auth0/auth0-spa-js": "^1.22.1",
    "@nguniversal/express-engine": "^14.0.2",
    "auth0": "^2.42.0",
    "bootstrap": "^5.1.3",
    "express": "^4.18.1",
    "hammerjs": "^2.0.8",
    "lodash-es": "^4.17.21",
    "ngx-bootstrap": "^8.0.0",
    "ngx-device-detector": "^3.0.0",
    "rxjs": "^7.5.5",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.0.4",
    "@angular-eslint/builder": "^14.0.0",
    "@angular-eslint/eslint-plugin": "^14.0.0",
    "@angular-eslint/eslint-plugin-template": "^14.0.0",
    "@angular-eslint/schematics": "^14.0.0",
    "@angular-eslint/template-parser": "^14.0.0",
    "@angular/cli": "~14.0.4",
    "@angular/compiler-cli": "^14.0.0",
    "@nguniversal/builders": "^14.0.2",
    "@types/express": "^4.17.13",
    "@types/facebook-js-sdk": "^3.3.5",
    "@types/jasmine": "^4.0.3",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^18.0.0",
    "eslint": "^8.18.0",
    "jasmine-core": "~4.1.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "typescript": "~4.7.2"
  },
  "browser": {
    "fs": false
  },
  "engines": {
    "node": "14.x"
  }
}