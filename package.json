{
  "name": "free-vote",
  "version": "12.3.3",
  "scripts": {
    "ng": "ng",
    "start": "ng serve -o --watch",
    "pwa": "http-server -p 8080 -c-1 dist/free-vote",
    "build": "ng build --configuration production",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "eslint . --ext .ts",
    "e2e": "ng e2e",
    "build:ssr": "ng build --configuration production && ng run free-vote:server:production",
    "serve:ssr": "node dist/free-vote/server/main.js",
    "ssr": "npm run build:ssr && npm run serve:ssr",
    "prettier-format": "prettier --config .prettierrc \"src/**/*.ts\" --write",
    "prerender-dynamic": "ng run free-vote:prerender --routes-file dynamic-routes.txt",
    "prerender": "ng run free-vote:prerender",
    "dev:ssr": "ng run free-vote:serve-ssr --port 7027",
    "start:heroku": "node dist/free-vote/server/main.js",
    "heroku-postbuild": "npm run build:ssr"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^12.0.5",
    "@angular/cdk": "^12.0.5",
    "@angular/common": "^12.0.5",
    "@angular/compiler": "^12.0.5",
    "@angular/core": "^12.0.5",
    "@angular/flex-layout": "^11.0.0-beta.33",
    "@angular/forms": "^12.0.5",
    "@angular/material": "^12.0.5",
    "@angular/platform-browser": "^12.0.5",
    "@angular/platform-browser-dynamic": "^12.0.5",
    "@angular/platform-server": "^12.0.5",
    "@angular/router": "^12.0.5",
    "@angular/service-worker": "^12.0.5",
    "@auth0/auth0-spa-js": "^1.13.6",
    "@nguniversal/express-engine": "^12.0.2",
    "bootstrap": "^5.0.1",
    "express": "^4.15.2",
    "hammerjs": "^2.0.8",
    "lodash-es": "^4.17.20",
    "ngx-bootstrap": "^6.2.0",
    "ngx-device-detector": "^2.0.5",
    "rxjs": "~6.6.0",
    "tslib": "^2.1.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^12.0.5",
    "@angular-eslint/builder": "12.1.0",
    "@angular-eslint/eslint-plugin": "12.1.0",
    "@angular-eslint/eslint-plugin-template": "12.1.0",
    "@angular-eslint/schematics": "12.1.0",
    "@angular-eslint/template-parser": "12.1.0",
    "@angular/cli": "^12.0.5",
    "@angular/compiler-cli": "^12.0.5",
    "@nguniversal/builders": "^12.0.2",
    "@types/express": "^4.17.0",
    "@types/jasmine": "^3.7.7",
    "@types/lodash-es": "^4.17.4",
    "@types/node": "^12.20.15",
    "@typescript-eslint/eslint-plugin-tslint": "^4.27.0",
    "@typescript-eslint/eslint-plugin": "^4.28.3",
    "@typescript-eslint/parser": "^4.28.3",
    "codelyzer": "^6.0.0",
    "eslint": "^7.30.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsdoc": "^35.3.2",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^3.4.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "prettier": "^2.3.2",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "typescript": "~4.2.0"
  },
  "browser": {
    "fs": false
  },
  "engines": {
    "node": "14.x"
  }
