{
  "name": "free-vote",
  "version": "15.8.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve -o --watch --host=localhost --configuration development --ssl true --ssl-key ssl/server.key --ssl-cert ssl/server.crt",
    "pwa": "http-server -p 7027 -c-1 dist/free-vote/browser",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "dev:ssr": "ng run free-vote:serve-ssr --port 7027",
    "serve:ssr": "node dist/free-vote/server/main.js",
    "build:ssr": "ng build && ng run free-vote:server",
    "prerender": "ng run free-vote:prerender",
    "lint": "eslint . --ext .ts",
    "e2e": "ng e2e",
    "ssr": "npm run build:ssr && npm run serve:ssr",
    "prettier-format": "prettier --config .prettierrc \"src/**/*.ts\" --write",
    "prerender-dynamic": "ng run free-vote:prerender --routes-file dynamic-routes.txt",
    "start:heroku": "node dist/free-vote/server/main.js",
    "heroku-postbuild": "npm run build:ssr"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.3.2",
    "@angular/cdk": "^17.3.2",
    "@angular/common": "17.3.2",
    "@angular/compiler": "^17.3.2",
    "@angular/core": "^17.3.2",
    "@angular/forms": "^17.3.2",
    "@angular/material": "^17.3.2",
    "@angular/platform-browser": "^17.3.2",
    "@angular/platform-browser-dynamic": "^17.3.2",
    "@angular/platform-server": "^17.3.2",
    "@angular/pwa": "^17.3.2",
    "@angular/router": "^17.3.2",
    "@angular/service-worker": "^17.3.2",
    "@angular/ssr": "^17.3.2",
    "@auth0/auth0-angular": "^2.0.1",
    "bootstrap": "^5.2.3",
    "express": "^4.18.2",
    "hammerjs": "^2.0.8",
    "lodash-es": "^4.17.21",
    "ngx-device-detector": "^7.0.0",
    "rxjs": "^7.5.5",
    "save": "^2.9.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.3.2",
    "@angular/cli": "^17.3.2",
    "@angular/compiler-cli": "^17.3.2",
    "@angular-eslint/builder": "^17.3.0",
    "@angular-eslint/eslint-plugin": "^17.3.0",
    "@angular-eslint/eslint-plugin-template": "^17.3.0",
    "@angular-eslint/schematics": "^17.3.0",
    "@angular-eslint/template-parser": "^17.3.0",
    "@types/express": "^4.17.14",
    "@types/facebook-js-sdk": "^3.3.5",
    "@types/jasmine": "~4.3.0",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^16.18.11",
    "eslint": "^8.26.0",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "typescript": "~5.4.3"
  },
  "resolutions": {
    "ua-parser-js": "0.7.28"
  },
  "browser": {
    "fs": false
  },
  "engines": {
    "node": "18.20.0",
    "npm": "9.6.5"
  }
}
